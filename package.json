{
  "name": "@sweetretry/ai-sdk-volcengine-adapter",
  "version": "0.1.1",
  "description": "Volcengine adapter for AI SDK",
  "license": "MIT",
  "author": "SweetRetry <zimin.zhang2000@gmail.com>",
  "keywords": [
    "ai",
    "ai-sdk",
    "volcengine",
    "doubao",
    "llm",
    "provider"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "swc src -d dist --config-file .swcrc --ignore '**/*.test.ts' --strip-leading-paths && tsc -p tsconfig.build.json",
    "build:js": "swc src -d dist --config-file .swcrc --ignore '**/*.test.ts' --strip-leading-paths",
    "build:types": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.3.107",
    "@types/node": "^25.0.3",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@ai-sdk/provider": "^3.0.2",
    "@ai-sdk/provider-utils": "^4.0.5",
    "zod": "^4.3.5"
  }
}

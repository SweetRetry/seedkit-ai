{
  "name": "@seed-kit/ai-sdk-provider",
  "version": "0.1.5",
  "description": "Seed adapter for AI SDK",
  "license": "MIT",
  "author": "SweetRetry <zimin.zhang2000@gmail.com>",
  "homepage": "https://github.com/SweetRetry/seed-kit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SweetRetry/seed-kit.git",
    "directory": "packages/ai-sdk-provider"
  },
  "bugs": {
    "url": "https://github.com/SweetRetry/seed-kit/issues"
  },
  "keywords": [
    "ai",
    "ai-sdk",
    "seed",
    "doubao",
    "llm",
    "provider"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "swc src -d dist --config-file .swcrc --ignore '**/*.test.ts' --strip-leading-paths && tsc -p tsconfig.build.json",
    "build:js": "swc src -d dist --config-file .swcrc --ignore '**/*.test.ts' --strip-leading-paths",
    "build:types": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "engines": {
    "node": ">=18"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "all"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.3.107",
    "@types/node": "^25.0.3",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@ai-sdk/provider": "^3.0.8",
    "@ai-sdk/provider-utils": "^4.0.5",
    "zod": "^4.3.5"
  }
}
